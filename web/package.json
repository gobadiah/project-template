{
  "name": "web-template",
  "license": "MIT",
  "scripts": {
    "dev": "babel-node bin/server.js",
    "clean": "rm -rf .next && rm -rf src/.next && rm -rf dist && rm -rf reports && rm -rf coverage && rm -rf src/static/build",
    "build": "npm run clean && next build src && NODE_ENV=production babel --copy-files --ignore node_modules,dist,__tests__ -d dist .",
    "start": "NODE_ENV=production node dist/bin/server.js",
    "lint": "eslint --ignore-path .gitignore --max-warnings=0 .",
    "lint:ci": "npm run lint --- --format junit -o reports/junit/js-lint-results.xml",
    "jest": "jest --config src/config/jest/index.js",
    "jest:ci": "JEST_JUNIT_OUTPUT=reports/junit/js-test-results.xml npm run jest -- --testResultsProcessor=\"jest-junit\" --ci --runInBand --coverage",
    "jest:coverage": "npm run jest -- --coverage",
    "jest:watch": "npm run jest -- --watch",
    "jest:watch:coverage": "npm run jest:coverage -- --watchAll",
    "test": "npm run build && npm run lint && npm run jest",
    "test:coverage": "npm run build && npm run lint && npm run jest:coverage",
    "test:ci": "npm run build && npm run lint:ci && npm run jest:ci"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-emotion": "^9.0.1",
    "babel-plugin-file-loader": "^1.0.0",
    "babel-plugin-inline-import": "^2.0.6",
    "babel-plugin-root-import": "^5.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-inline-env-vars": "^1.0.1",
    "babel-preset-env": "^1.6.1",
    "cookie": "^0.3.1",
    "emotion": "^9.0.2",
    "emotion-server": "^9.0.2",
    "express": "^4.16.2",
    "file-loader": "^1.1.10",
    "i18next": "^10.4.1",
    "i18next-browser-languagedetector": "^2.1.0",
    "i18next-express-middleware": "^1.0.10",
    "i18next-node-fs-backend": "^1.0.0",
    "i18next-xhr-backend": "^1.5.1",
    "immutable": "^3.8.2",
    "json-api-denormalizer": "^1.0.1",
    "lodash": "^4.17.5",
    "next": "^5.0.0",
    "next-redux-wrapper": "^1.3.5",
    "next-routes": "^1.3.0",
    "nprogress": "^0.2.0",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth": "^1.0.0",
    "prop-types": "^15.6.0",
    "raven": "^2.4.1",
    "raven-js": "^3.22.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-emotion": "^9.0.2",
    "react-i18next": "^7.4.0",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-devtools-extension": "^2.13.2",
    "redux-json-api": "^2.0.0-beta.5",
    "redux-thunk": "^2.2.0",
    "serve-favicon": "^2.4.5",
    "webpack": "^4.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.1",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^3.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.12.2",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "jest": "^22.4.2",
    "jest-emotion": "^9.0.0",
    "jest-junit": "^3.6.0",
    "jsdom": "^11.6.2",
    "jsdom-global": "^3.0.2",
    "react-test-renderer": "^16.2.0",
    "supertest": "^3.0.0"
  },
  "browser": {
    "raven": "raven-js"
  }
}
